{
    "name": "avikarsha/yii2-datatables",
    "description": "Datatables",
    "type": "yii2-extension",
    "keywords": ["yii2","extension"],
    "license": "MIT",
    "authors": [
        {
            "name": "Avikarsha Saha",
            "email": "avikarsha.saha@gmail.com"
        }
    ],
    "require": {
        "yiisoft/yii2":"*",
        "bower-asset/jquery":">= 1.7.0",
        "bower-asset/datatables":">= 1.9.4",
        "bower-asset/datatables-bootstrap3":"*",        
        "bower-asset/datatables-tabletools":"*"        
    },
    "autoload": {
        "psr-4": {
            "avikarsha\\datatables\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "bower-asset-library": "vendor/bower"
        }
    }
}
